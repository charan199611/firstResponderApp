{
  "name": "ResponderApp",
  "version": "9.0.2",
  "author": "Ionic Framework",
  "main": "index.js",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "ng": "ng",
    "start": "electron .",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:electron": "ng build --base-href ./",
    "build:electron:dev": "npm run build:electron -- -c dev",
    "build:electron:prod": "npm run build:electron -- -c production",
    "electron:dev": "npm-run-all -p  electron:serve",
    "electron:dev:debug": "npm-run-all -p ng:serve electron:serve:debug",
    "electron:dev:debug-live": "npm-run-all -p ng:serve electron:serve:debug-live",
    "electron:serve": "wait-on http-get://localhost:4200/ && run-electron . --serve",
    "electron:serve:debug": "wait-on http-get://localhost:4200/ && run-electron --inspect-brk=9229 . --serve",
    "electron:serve:debug-live": "wait-on http-get://localhost:4200/ && nodemon --watch electron.js --exec run-electron --inspect-brk=9229 . --serve",
    "electron:local": "npm run build:electron:prod && run-electron .",
    "electron:linux": "npm run build:electron:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:electron:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:electron:prod && npx electron-builder build --mac",
    "ionic": "npm run ng:serve:web",
    "emulate:ios-dev": "ionic cordova emulate ios --l -- --buildFlag='-UseModernBuildSystem=0'",
    "run:ios-dev": "ionic cordova run ios --l --device -- --buildFlag='-UseModernBuildSystem=0'",
    "emulate:ios": "ionic cordova emulate ios -- --buildFlag='-UseModernBuildSystem=0'",
    "run:ios": "ionic cordova run ios --device -- --buildFlag='-UseModernBuildSystem=0'",
    "release:ios": "ionic cordova build ios --prod --release -- --buildFlag='-UseModernBuildSystem=0'",
    "emulate:android-dev": "ionic cordova emulate android --l",
    "run:android-dev": "ionic cordova run android --device --l",
    "emulate:android": "ionic cordova emulate android",
    "run:android": "ionic cordova run android --device",
    "release:android": "ionic cordova build android --prod --release ",
    "ng:serve": "ng serve",
    "ng:serve:web": "ng serve -o",
    "xcode": "open -a Xcode platforms/ios/",
    "buildWin": "electron-packager AppDir AppName --platform=win32 --arch=x64 --electronVersion=8.0.0 --app-version=1.0.0 --build-version=2.0.0  --prune=false --overwrite=true && electron ./package.js",
    "postinstall": "electron-builder install-app-deps",
    "deploy": "electron-builder build --win --publish always"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^8.2.0",
    "@angular/common": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^5.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "chart.js": "^2.9.4",
    "cordova-plugin-startapp": "^0.1.6",
    "core-js": "^3.0.0",
    "electron-icon-builder": "^2.0.1",
    "electron-push-receiver": "^2.1.3",
    "electron-reload": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^6.0.0",
    "electron-updater": "^4.3.5",
    "file-saver": "^2.0.5",
    "jquery": "^3.5.1",
    "jsmpeg-player": "^3.0.3",
    "lamejs": "1.2.0",
    "linebyline": "^1.3.0",
    "mic-recorder-to-mp3": "^2.2.2",
    "moment": "^2.28.0",
    "mqtt": "^4.2.6",
    "ng2-charts": "^2.4.2",
    "ng2-mqtt": "^0.1.2",
    "ngx-mqtt": "^7.0.14",
    "node-machine-id": "^1.1.12",
    "node-rtsp-stream": "0.0.9",
    "npm-upgrade": "^2.0.4",
    "numeral-systems": "^2.0.0",
    "random-access-file": "^2.1.4",
    "read-excel-file": "^4.1.0",
    "readline": "^1.3.0",
    "recordrtc": "^5.6.1",
    "rxjs": "~6.5.1",
    "simple-pdf-viewer": "^2.0.3",
    "sqlite3": "^5.0.0",
    "translate": "^1.1.2",
    "tslib": "^1.13.0",
    "xlsx": "^0.16.9",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.29",
    "@angular/cli": "^8.3.29",
    "@angular/compiler": "~8.2.14",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@ionic/angular-toolkit": "^2.3.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.5.5",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.2",
    "electron": "^8.5.1",
    "electron-builder": "^21.2.0",
    "electron-rebuild": "^1.11.0",
    "electron-winstaller": "^4.0.1",
    "electron-wix-msi": "^2.2.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "npm-run-all": "4.1.5",
    "protractor": "^5.4.4",
    "run-electron": "1.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "wait-on": "4.0.0"
  },
  "description": "First Responder App",
  "config": {
    "ionic_bundler": "webpack",
    "ionic_webpack": "./extra-webpack.config.js",
    "forge": {
      "packagerConfig": {
        "asar": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charan199611/firstResponderApp.git"
  }
}
